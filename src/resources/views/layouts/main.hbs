        <meta charset='utf-8' />
        <title>Quản lý kho</title>
        <meta name='viewport' content='width=device-width, initial-scale=1.0' />
        <meta
            content='Responsive bootstrap 4 admin template'
            name='description'
        />
        <meta content='Coderthemes' name='author' />
        <meta http-equiv='X-UA-Compatible' content='IE=edge' />
        <!-- App favicon -->
        <link rel='shortcut icon' href='\Uplon\assets\images\favicon.ico' />

        <!-- App css -->
        <link
            href='\Uplon\assets\css\bootstrap.min.css'
            rel='stylesheet'
            type='text/css'
            id='bootstrap-stylesheet'
        />
        <link
            href='\Uplon\assets\css\icons.min.css'
